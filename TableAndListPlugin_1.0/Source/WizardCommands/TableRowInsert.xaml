<Window x:Name="frmTableRowInsert" x:Class="TableListCommands.TableRowInsert"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Insert Row In Table" Height="476.682" Width="699.46" Loaded="frmTableCSVGrid_Loaded" WindowStartupLocation="CenterScreen"
        Closing="frmTableCSVGrid_Closing" 
        >
    <Grid>
        <DataGrid x:Name="gridCSVContent" Margin="15,33,10,22" RowHeaderWidth="30" Sorting="gridCSVContent_Sorting"
                   />
        <Button x:Name="btnInsertRow" Content="Insert Row" Margin="0,6,90,0" Click="btnInsertRow_Click_1"  HorizontalAlignment="Right" Width="80" VerticalAlignment="Top"/>
        <Button Content="OK" HorizontalAlignment="Right" Margin="0,6,10,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <TextBox x:Name="txtRow" Height="22" Margin="15,6,184,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
    </Grid>
</Window>
