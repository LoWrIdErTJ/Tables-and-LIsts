<Window x:Name="frmTableCSVGrid" x:Class="TableListCommands.TableCSVGrid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit Tabular CSV" Height="476.682" Width="699.46" Loaded="frmTableCSVGrid_Loaded" WindowStartupLocation="CenterScreen"
        Closing="frmTableCSVGrid_Closing" 
        >
    <Grid>
        <DataGrid x:Name="gridCSVContent" Margin="15,42,10,22" RowHeaderWidth="30" Sorting="gridCSVContent_Sorting"
                   />
        <Button x:Name="btnDeleteRow" Content="Delete Row" Margin="15,6,0,0" VerticalAlignment="Top" Click="btnDeleteRow_Click" HorizontalAlignment="Left" Width="106"/>
        <Button x:Name="btnDeleteColumn" Content="Delete Column" Margin="141,6,0,0" Click="btnDeleteColumn_Click" HorizontalAlignment="Left" Width="106" VerticalAlignment="Top"/>
        <Button Content="OK" HorizontalAlignment="Right" Margin="0,6,10,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
    </Grid>
</Window>
